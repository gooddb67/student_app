<h1>New Student</h1>

  <% if @student.errors.any? %>
  <ul>
    <% @student.errors.full_messages.each do |message| %>
    <li>
      <%= message %>  </li>
    <% end %>

  </ul>
  <% end %>


<%= form_for @student do |f| %>
  <%=f.label :student %>
  <%=f.text_field :student %>
  <%=f.submit "Create Student" %>
<% end %>
